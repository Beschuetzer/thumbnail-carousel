{
  "name": "react-thumbnail-carousel",
  "version": "1.0.3",
  "description": "A fully-customizable React carousel with various viewing modes and video support",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:sass && rollup -c --bundleConfigAsCjs",
    "build:sass": "sass src/sass/main.scss src/css/style.css",
    "lint": "npx prettier --write **/*",
    "lint:check": "npx prettier --check **/*",
    "publish": "npm run build && npm publish --access public",
    "watch:sass": "sass src/sass/main.scss src/css/style.css --watch",
    "test": "jest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [
    "carousel",
    "react",
    "thumbnail"
  ],
  "author": "Adam Major",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Beschuetzer/thumbnail-carousel.git"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.2",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/react-webpack5": "^7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.7",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.36",
    "@types/react-dom": "^18.2.14",
    "@types/react-redux": "^7.1.28",
    "autoprefixer": "^10.4.13",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "concat": "^1.0.3",
    "concurrently": "7.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "live-server": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.1.0",
    "prettier": "3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^4.3.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.69.5",
    "storybook": "^7.5.3",
    "typescript": "^5.2.2"
  }
}
